{
    "parser": "@babel/eslint-parser",
    "plugins": [
        "prettier"
    ],
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": ["airbnb-base", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "prettier/prettier": ["error"]
    },
    "globals": {
        "__rootdir": "readonly",
        "M": "readonly"
    }
}